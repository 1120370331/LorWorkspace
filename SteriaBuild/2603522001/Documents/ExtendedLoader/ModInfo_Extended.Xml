    <AtkEffectPivotInfo>
	<!-- Used to indicate where the attack effect is occured from, which makes it more appropriate for characters to use vanilla cards -->
	<!-- 用来指示特效生成的位置，这会让皮肤在使用原版特效时表现更好 -->
